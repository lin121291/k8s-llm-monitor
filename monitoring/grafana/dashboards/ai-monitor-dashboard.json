{
  "dashboard": {
    "id": null,
    "title": "AI Service Monitor Dashboard",
    "tags": ["ai-monitor", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"ai-monitor-services\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "Down", "color": "red"},
                  "1": {"text": "Up", "color": "green"}
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{service}} - {{method}}"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "{{service}} - Errors"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 4,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service}} - 95th percentile"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 5,
        "title": "Anomalies Detected",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(anomalies_detected_total[1h])",
            "legendFormat": "Last Hour"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 12}
      },
      {
        "id": 6,
        "title": "Scaling Events",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(scaling_events_total[1h])",
            "legendFormat": "Last Hour"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 12}
      },
      {
        "id": 7,
        "title": "Pod Replicas",
        "type": "graph",
        "targets": [
          {
            "expr": "kube_deployment_status_replicas{namespace=\"ai-monitor\"}",
            "legendFormat": "{{deployment}} - Current"
          },
          {
            "expr": "kube_deployment_spec_replicas{namespace=\"ai-monitor\"}",
            "legendFormat": "{{deployment}} - Desired"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{namespace=\"ai-monitor\"}[5m]) * 100",
            "legendFormat": "{{pod}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "max": 100
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18}
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{namespace=\"ai-monitor\"} / container_spec_memory_limit_bytes{namespace=\"ai-monitor\"} * 100",
            "legendFormat": "{{pod}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "max": 100
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18}
      }
    ]
  }
}